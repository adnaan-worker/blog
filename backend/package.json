{
  "name": "blog-node-service",
  "version": "3.0.0",
  "description": "博客系统node后端服务",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:seed": "sequelize-cli db:seed:all",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "format:all": "node scripts/format-all.js",
    "format:check": "prettier --check \"**/*.{js,json,md}\"",
    "format:models": "prettier --write \"models/**/*.js\"",
    "format:controllers": "prettier --write \"controllers/**/*.js\"",
    "format:services": "prettier --write \"services/**/*.js\"",
    "format:routes": "prettier --write \"routes/**/*.js\"",
    "format:utils": "prettier --write \"utils/**/*.js\"",
    "format:config": "prettier --write \"config/**/*.js\""
  },
  "dependencies": {
    "@langchain/anthropic": "^1.1.3",
    "@langchain/community": "^1.0.5",
    "@langchain/core": "^1.1.0",
    "@langchain/google-genai": "^2.0.0",
    "@langchain/openai": "^1.1.3",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.63.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "helmet": "^6.2.0",
    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^1.1.1",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "mysql2": "^3.14.0",
    "passport": "^0.7.0",
    "passport-gitee": "^0.0.1",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "sequelize": "^6.37.7",
    "slugify": "^1.6.6",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ua-parser-js": "^2.0.6",
    "uuid": "^9.0.1",
    "vm2": "^3.10.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "eslint": "^9.37.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "sequelize-cli": "^6.6.2"
  },
  "keywords": [
    "blog",
    "node",
    "express",
    "mysql",
    "redis"
  ],
  "author": "adnaan",
  "license": "ISC",
  "_moduleAliases": {
    "@": ".",
    "@services": "./services",
    "@utils": "./utils",
    "@config": "./config",
    "@models": "./models",
    "@controllers": "./controllers",
    "@middlewares": "./middlewares",
    "@routes": "./routes"
  }
}
